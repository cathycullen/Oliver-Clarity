
<div class="row-fluid col-md-6 clearfix">
  <div class="span4 well">

    <%= simple_form_for [@user, @webdoc]  do |f| %>

      <% if @webdoc.errors.any? %>
        <div id="error_explanation">
          <h2><%= pluralize(@webdoc.errors.count, "error") %> prohibited this webdoc from being saved:</h2>
          <ul>
          <% @webdoc.errors.full_messages.each do |msg| %>
            <li><%= msg %></li>
          <% end %>
          </ul>
        </div>
      <% end %>
    <fieldset >
      <legend><%= operation %> webdoc</legend>
        <%= f.input :url %>
        <%= f.input :state %>
        <%= f.input :children %>
        <%= f.input :products %>
        <%= f.input :similar %>
        <%= f.file_field :file %>
      <div class="actions">
        <%= f.button :submit %>
        <%= link_to 'Cancel', user_path(@user), class: "btn btn-lg btn-danger"%>
      </div>
    </fieldset >
    <% end %>
  </div>
</div>
</div>
